#autoSuggestList{border:1px solid #b3b3b3; background-color:#ffffff; height:auto; text-align:left;padding:0px 6px 6px; border-radius:3px;}

#autoSuggestList li{margin-top:6px; cursor:pointer; line-height:24px; color:#363636;; padding-right:10px}
#autoSuggestList li.suggest_title{border-top:none; cursor:default;}
#autoSuggestList li img{display:block; float:left; margin-right:5px;}
#autoSuggestList li.on{background-color:#eeeeee}
