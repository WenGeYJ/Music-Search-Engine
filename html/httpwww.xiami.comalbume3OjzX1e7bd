		e.preventDefault();
		$(this).css('z-index','6').children('span').show();
	});

	$("#page").delegate('.song_menu','mouseout',function(e){
		e.preventDefault();
		$(this).css('z-index','5').children('span').hide();
	});
	

	$(".bt_cdgears").mouseover(function(){$(this).addClass('active').next().removeClass('hidden').show();});

