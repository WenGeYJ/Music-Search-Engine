		$(function(){
			$('.comment_acts').hover(function(){
				$(this).addClass('acts_hover');
			},function(){
				$(this).removeClass('acts_hover');
			});
			
			$(".comment_item_main .title .more").click(function(){
				var self = $(this);
				var CommetId = self.data("commetid");
				if(self.hasClass("open")){
					self.removeClass("open").addClass("close");
					this.innerHTML = "展开";
					this.title = "展开";
				}else{
					self.removeClass("close").addClass("open");
					this.innerHTML = "收起";
					this.title = "收起";
					
					var view = $("#review_"+CommetId+"_full").find('em').eq(0).html();
					$("#review_"+CommetId+"_full").find('em').eq(0).html(parseInt(view)+1);					
					$.getJSON('/ajax/add-comment-views',{'id':CommetId},function(data){
						
					});				
				}
				$("#review_"+CommetId+"_full").toggle();
				$("#review_"+CommetId+"_short").toggle();
			});
		});
