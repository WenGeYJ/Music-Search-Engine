<tr data-needpay="{%=o.data[i].needpay%}" data-playstatus="{%=o.data[i].playstatus%}" data-downloadstatus="{%=o.data[i].downloadstatus%}" data-json="{%=o.data[i].downloadjson%}" {%if (i%2){ %}{% }else{ %} class="bg_grey" {% } %}>
