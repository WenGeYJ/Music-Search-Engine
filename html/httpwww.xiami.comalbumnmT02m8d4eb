	*font-style: normal;
	position: absolute;
	left: -2px;
	color: #f60;
}
#sidebutton a:hover {
	border-color: #E06E16;
	background-color: #f60;
	color: #FFF;
	text-align: center;
	text-decoration: none;
}
#sidebutton a:hover .sidebutton_icon {
	display: none;
}
#sidebutton a:hover span {
	display: block;
}
#sidebutton .sidebutton_qrcode:hover .sidebutton_code {
	display: block;
}
.t_hide {
  display: none;
}
.t_show {
  display: block;
