<a class="song_digg" href="javascript:void(0)" title="推荐" onclick="recommend('{%=o.data[i].song_id%}','32')"><span>推荐</span></a>
<a class="song_toclt" href="javascript:void(0)" title="添加{%=o.data[i].songName%}到精选集" onclick="collect('{%=o.data[i].song_id%}');"><span>添加到am精选集</span></a>
{% if (o.data[i].downloadstatus>0){ %}
<a class="song_download" href="javascript:void(0)" title="{%=o.data[i].songName%}下载" onclick="xm_download('{%=o.data[i].song_id%}', 'song' ,this);"><span>下载</span></a>
{% } %}
