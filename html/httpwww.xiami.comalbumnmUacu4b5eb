<script type="text/x-tmpl" id="tmpl-cdsong">
{% for (var i=0; i<o.data.length; i++) { %}
{% if (o.data[i].track == 1) { %}
<tr><td style="border: none;padding:0;" colspan="5"><strong class="trackname" style=" border-bottom: 1px solid #E5E5E5;padding: 10px 5px;">disc {%=o.data[i].cdSerial%}</strong></td></tr>
{% } %}
